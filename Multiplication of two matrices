#include<stdio.h>

void main() 
{
    int a[3][3] = {1, 2, 3,  // a[0][0] a[0][1] a[0][2]
                   1, 2, 3,  // a[1][0] a[1][1] a[1][2]
                   1, 2, 3}; // a[2][0] a[2][1] a[2][2]

    int b[3][3] = {1, 2, 3,  // b[0][0] b[0][1] b[0][2]
                   1, 2, 3,  // b[1][0] b[1][1] b[1][2]
                   1, 2, 3}; // b[2][0] b[2][1] b[2][2]

    int c[3][3];

    // 11 =  a[0][0] * b[0][0] + a[0][1] * b[1][0] + a[0][2] * b[2][0];
    // 12 =  a[0][0] * b[0][1] + a[0][1] * b[1][1] + a[0][2] * b[2][1]; 
    // 13 =  a[0][0] * b[0][2] + a[0][1] * b[1][2] + a[0][2] * b[2][2];
    //         i  j      j  k      i  j      j  k      i  j      j  k  

    for(int i = 0; i<3; i++)
    {
        for(int j = 0; j < 3; j++)
        {
            c[i][j] = 0;
            for(int k = 0; k < 3; k++)
            { 
                c[i][j] += a[i][k]*b[k][j];
            }
        }
    }

    for(int i=0; i<3; i++)
    {
        for(int j=0; j<3; j++)
        {
            printf("%d  ", c[i][j]);
        }
        printf("\n");
    }
}
